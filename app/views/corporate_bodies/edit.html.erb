<% @pagetitle = "Edit Corporate Body: " + @corporate_body.name %>

<h1>Edit corporate body: <%= @corporate_body.name %></h1>

<p>

<%= link_to 'Delete', @corporate_body, :confirm => 'Are you sure?', :method => :delete %>

</p>

<% form_for(@corporate_body) do |f| %>
<%= f.error_messages %>

<%= render(:partial => 'form', :object => f) %>

<p>
<%= f.submit "Update" %>
</p>
<% end %>

<%= link_to 'Show', @corporate_body %> |
<%= link_to 'Back', corporate_bodies_path %>
